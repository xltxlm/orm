{
  "name": "xltxlm/orm",
  "description": "ORM类库,根据数据库直接生成类",
  "license": "Apache 2.0",
  "authors": [
    {
      "name": "夏琳泰",
      "email": "xltxlm@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-pdo": "*",
    "overtrue/pinyin": "~3.0",
    "monolog/monolog": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "*"
